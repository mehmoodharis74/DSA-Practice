#we have to solve this question in onepass algo and constant space
def sortColors(nums):
        rcount=wcount=bcount=0
        for x in nums:
            if x == 0:
                rcount +=1
            elif x == 1:
                wcount += 1
            else:
                bcount +=1
        wcount += rcount
        bcount += wcount
        for x in range(len(nums)):
            if x < rcount:
                nums[x] = 0
            elif x < wcount:
                nums[x] = 1
            else: 
                nums[x] = 2
        